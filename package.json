{
  "name": "totp-online",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:android": "bubblewrap build",
    "update:android": "bubblewrap update",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run totp-online:serve-ssr",
    "serve:ssr": "node dist/totp-online/server/main.js",
    "build:ssr": "ng build && ng run totp-online:server",
    "prerender": "ng run totp-online:prerender",
    "pwa:serve": "http-server -p 8080 -c-1 dist/totp-online/browser"
  },
  "private": true,
  "prettier": "@tobysmith568/prettier-config",
  "dependencies": {
    "@angular/animations": "~12.0.2",
    "@angular/cdk": "^12.1.0",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/platform-server": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@angular/service-worker": "~12.0.2",
    "@nguniversal/express-engine": "^12.0.2",
    "express": "^4.15.2",
    "jssha": "^3.2.0",
    "ng-circle-progress": "^1.6.0",
    "ng-qrcode": "^5.0.0",
    "ngx-scrollbar": "^7.5.6",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^12.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.0.2",
    "@bubblewrap/cli": "^1.13.6",
    "@nguniversal/builders": "^12.0.2",
    "@tobysmith568/prettier-config": "^1.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "http-server": "^13.0.2",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  },
